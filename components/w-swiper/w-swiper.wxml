<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" circular autoplay interval='3000' indicator-dots indicator-active-color="#e43130">
  <block wx:for="{{list}}" wx:key="id">
    <swiper-item class="swiper-item">
      <image src="{{item.image || item}}" mode="widthFix"></image>
    </swiper-item>
  </block> 
</swiper>
